{
  "manifest_version": 2,
  "name": "Super Search",
  "version": "1.0.0",

  "browser_action": {
    "default_title": "Super Search"
  },

  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.google.com/*", "http://www.google.com/*"],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ]
}
